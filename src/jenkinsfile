pipeline {
   agent any
   
   stages {
      
      stage('Compile java program ') {
         
         steps {
            
            sh 'javac -d bin src/*.java'
         }
      }
      
      stage('Run Java Program') {
         
         steps {
            sh 'java /Practice_Again/src/Practice_main.java'
         }
      }
   }
}